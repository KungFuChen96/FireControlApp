  <appSettings>
    <!--Neware MES调度地址-->
    <add key="BatMesUrl" value="http://192.168.5.80:8102/" />
    <!--Neware MES SDK 日志目录-->
    <add key="BatMesSdkLogDir" value="./Logs/NewareMesSdk/" />
    <add key="intervalFire" value="5" />
    <add key="intervalFault" value="8" />
    <add key="ColCountLine_1" value="7" />
    <add key="RowCountLine_1" value="4" />
    <add key="ColCountLine_2" value="7" />
    <add key="RowCountLine_2" value="4" />
